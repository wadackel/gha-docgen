{
  "extends": [
    "config:js-lib",
    "group:typescript-eslintMonorepo",
    "group:commitlintMonorepo",
    ":semanticCommits",
    ":semanticCommitTypeAll(refactor)",
    ":semanticCommitScopeDisabled"
  ],
  "labels": ["A-dependencies"],
  "rebaseLabel": "S-needs-rebase",
  "major": {
    "stabilityDays": 7
  },
  "minor": {
    "stabilityDays": 3
  },
  "patch": {
    "stabilityDays": 2
  },
  "prConcurrentLimit": 0
}
